<div class="modal modal_confirmation fade bs-example-modal-sm" id="sysConfigConfirm" tabindex="1" role="dialog" aria-labelledby="mySmallModalLabel" data-backdrop="static" data-keyboard="false" hidden>
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-body">Are you sure you want to remove?</div>
            <div class="modal-footer">
                <button type="button" id="Yes" class="btn btn-primary">Yes, remove</button>
                <button type="button" id="No"  class="btn btn-default btn-xs btn_cancel dontRemove">No, don't remove</button>
            </div>
        </div>
    </div>
</div>
