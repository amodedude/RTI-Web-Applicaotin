            <span>@Model.Source1Statistics.MaxConductivity</span><br />
    
            @Html.LabelFor(model => model.Source1Statistics.MinConductivity)
            <span>@Model.Source1Statistics.MinConductivity</span><br />
    
            @Html.LabelFor(model => model.Source1Statistics.MedianValue)
            <span>@Model.Source1Statistics.MedianValue</span><br />
    
            @Html.LabelFor(model => model.Source1Statistics.ModalValue)
