@model RTI.ModelingSystem.Core.Models.ConductivityViewModel

@{
    ViewBag.Title = "SourceStatistics";
}

<h2>SourceStatistics</h2>

<div>

        <div>
            
            @Html.RadioButtonFor(model => model.IsFemale, "true") Male
            @Html.RadioButtonFor(model => model.IsFemale, "false") Female

        </div>
